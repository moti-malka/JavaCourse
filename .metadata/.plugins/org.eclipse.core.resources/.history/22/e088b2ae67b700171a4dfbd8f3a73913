package guessTheNumber;

import java.awt.geom.CubicCurve2D;
import java.util.Random;

import java.util.Scanner;

public class guessTheNumber {

	public static void main(String[] args) {

		int clock = 7;

		int add = 5;

		if (add < 24) {
			clock = add;
			System.out.println(clock);
		} else if (add >= 24) {

			clock = (add % 24);
			System.out.println(clock);
		}

	}
}