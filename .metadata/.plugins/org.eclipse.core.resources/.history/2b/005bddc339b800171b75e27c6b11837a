package ClockPackage;

public class AppClock {

	public static void main(String[] args) {

		Clock motiClock = new Clock(12, 45, 57, 10, "AMPM", false);
		
				
		motiClock.addHours(1);
		motiClock.addMinutes(20);
		motiClock.addSecound(10);
		motiClock.addmilisecound(50);
		
		motiClock.printTime(true);
		

	}
}