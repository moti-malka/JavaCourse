package guessTheNumber;

import java.awt.geom.CubicCurve2D;
import java.util.Random;

import java.util.Scanner;

public class guessTheNumber {

	public static void main(String[] args) {

		int clock = 60;

		int add = 61;

		if (add < 60) {
			clock = add;
			System.out.println(clock);
		} else if (add >= 60) {

			clock = (add % 60);
			System.out.println(clock);
		}

	}
}